<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="cs.kontratazioa">
<body>
    <div metal:fill-slot="main">
        <tal:main-macro metal:define-macro="main">
          <h1 class="documentFirstHeading"> 
             <tal:content content="here/Title"></tal:content>
	  </h1>
          <table class="grid orokorra">
          <tal:condition condition="here/getFile_number">
          <tr>
          <th i18n:translate="file_number"></th>
          <td tal:content="here/getFile_number"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getContract_type">
          <tr>
          <th i18n:translate="contract_type"></th>
          <td tal:content="here/getContract_type"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getProcess">
          <tr>
          <th i18n:translate="process"></th>
          <td tal:content="here/getProcess"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getIzapidea">
          <tr>
          <th i18n:translate="izapidea"></th>
          <td tal:content="here/getIzapidea"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getOrganization">
          <tr>
          <th i18n:translate="organization"></th>
          <td tal:content="here/getOrganization"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getKontratazio_organoa">
          <tr>
          <th i18n:translate="kontratazio_organoa"></th>
          <td tal:content="here/getKontratazio_organoa"></td>
          </tr>
          </tal:condition>
          
          <tal:condition define="last_date here/getPublished_date" condition="last_date">
          <tr>
          <th i18n:translate="published_date"></th>
          <td tal:content="python: here.toLocalizedTime(last_date, long_format=1)"></td>
          </tr>
          </tal:condition>

          <tal:condition condition="here/getLizitazio_aurrekontua">
          <tr>
          <th i18n:translate="lizitazio_aurrekontua"></th>
          <td tal:content="here/getLizitazio_aurrekontua"></td>
          </tr>
          </tal:condition>
          
          <tal:condition condition="python: here.getAttach2().size">
          <tr>
          <th i18n:translate="attach2"></th>
          <td><span metal:use-macro="python:here.widget('attach2',
 	                             mode='view')" /></td>
          </tr>
          </tal:condition>

          <tal:condition condition="python: here.getAttach3().size">
          <tr>
          <th i18n:translate="attach3"></th>
          <td><span metal:use-macro="python:here.widget('attach3',
 	                             mode='view')" /></td>
          </tr>
          </tal:condition>

         

          <tal:condition condition="here/getDocumentation">
          <tr>
          <th i18n:translate="documentation"></th>
          <td tal:content="structure here/getDocumentation"></td>
          </tr>
          </tal:condition>
          
          <tal:condition condition="python: here.getAttach4().size">
          <tr>
          <th i18n:translate="attach4"></th>
          <td><span metal:use-macro="python:here.widget('attach4',
 	                             mode='view')" /></td>
          </tr>
          </tal:condition>
          
          <tal:condition condition="python: here.getAttach1_information()">
          <tr>
          <th i18n:translate="attach1"></th>
          <td><span metal:use-macro="python:here.widget('attach1',
 	                             mode='view')" /><br /><span tal:content="structure here/getAttach1_information"></span><br /></td>
          </tr>
          </tal:condition>


          <tal:condition define="last_date here/getLast_date" condition="last_date">
          <tr>
          <th i18n:translate="last_date"></th>
          <td tal:content="python: here.toLocalizedTime(last_date, long_format=1)"></td>
          </tr>
          </tal:condition>
          
           <tal:condition condition="python: here.aq_parent.getContact_information()">
          <tr>
          <th i18n:translate="contact_information"></th>
          <td tal:content="structure python: here.aq_parent.getContact_information()"></td>
          </tr>
          </tal:condition>

          <tal:condition define="last_date here/getEskaintza_ekonomikoa_date" condition="last_date">
          <tr>
          <th i18n:translate="last_date"></th>
          <td tal:content="python: here.toLocalizedTime(last_date, long_format=1)"></td>
          </tr>
          </tal:condition>
         </table>

          <table class="grid behineko" tal:condition="python: here.getBehin_behineko_file().size">
          
          <tr>
          <th i18n:translate="behin_behineko_file" colspan="2" class="goiburua"></th>
          </tr>
          <tr tal:define="adjudikazio_date here/getBehin_behineko_adjudikazio_date" tal:condition="adjudikazio_date">
          <th i18n:translate="behin_behineko_adjudikazio_date"></th>
          <td tal:content="python: here.toLocalizedTime(adjudikazio_date, long_format=1)"></td>
          </tr>
          <tr><th i18n:translate="akordioa"></th><td><span metal:use-macro="python:here.widget('behin_behineko_file',
 	                             mode='view')" /></td>
          </tr>
          
          <tr tal:condition="here/getBehin_behineko_adjudikazioduna">
          <th i18n:translate="behin_behineko_adjudikazioduna"></th>
          <td tal:content="structure here/getBehin_behineko_adjudikazioduna"></td>
          </tr>

          <tr tal:condition="here/getBehin_behineko_adjudikazioaren_zenbatekoa">
          <th i18n:translate="behin_behineko_adjudikazioaren_zenbatekoa"></th>
          <td tal:content="structure here/getBehin_behineko_adjudikazioaren_zenbatekoa"></td>
          </tr>
          

          <tr tal:define="profile_date here/getBehin_behineko_profile_date" tal:condition="profile_date">
          <th i18n:translate="behin_behineko_profile_date"></th>
          <td tal:content="python: here.toLocalizedTime(profile_date, long_format=1)"></td>
          </tr>

          </table>


          <table class="grid betiko" tal:condition="python: here.getBehin_betiko_file().size">
          
          <tr>
          <th i18n:translate="behin_betiko_file"  colspan="2" class="goiburua"></th>
          </tr>
          <tr tal:define="adjudikazio_date here/getBehin_betiko_adjudikazio_date" tal:condition="adjudikazio_date">
          <th i18n:translate="behin_betiko_adjudikazio_date"></th>
          <td tal:content="python: here.toLocalizedTime(adjudikazio_date, long_format=1)"></td>
          </tr>
          <tr><th i18n:translate="akordioa"></th><td><span metal:use-macro="python:here.widget('behin_betiko_file',
 	                             mode='view')" /></td>
          </tr>
          
          <tr tal:condition="here/getBehin_betiko_adjudikazioduna">
          <th i18n:translate="behin_betiko_adjudikazioduna"></th>
          <td tal:content="structure here/getBehin_betiko_adjudikazioduna"></td>
          </tr>

          <tr tal:condition="here/getBehin_betiko_adjudikazioaren_zenbatekoa">
          <th i18n:translate="behin_betiko_adjudikazioaren_zenbatekoa"></th>
          <td tal:content="structure here/getBehin_betiko_adjudikazioaren_zenbatekoa"></td>
          </tr>
          

          <tr tal:define="profile_date here/getBehin_betiko_profile_date" tal:condition="profile_date">
          <th i18n:translate="behin_betiko_profile_date"></th>
          <td tal:content="python: here.toLocalizedTime(profile_date, long_format=1)"></td>
          </tr>

          </table>

          
          
        </tal:main-macro>
    </div>
</body>
</html>
